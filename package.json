{
  "name": "switchsetup",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "build-win": "webpack && pkg -t node14-win --compress Brotli --out-path bin ./dist/main.js",
    "build-mac": "webpack && pkg -t node14-macos-arm64 --compress Brotli --out-path bin ./dist/main.js",
    "clean": "tsc --build --clean",
    "start": "ts-node index.ts",
    "cmd": "ts-node cmd.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "node-loader": "^2.0.0",
    "pkg": "^5.8.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "pkg": {
    "scripts": "dist/main.js",
    "assets": [
      "node_modules/@serialport/binding*/**/*"
    ],
    "targets": [
      "node18-macos-Arm64",
      "node18-win-x64"
    ],
    "outputPath": "bin",
    "options": [
      "experimental-modules"
    ]
  },
  "dependencies": {
    "@types/prompts": "^2.4.1",
    "chalk": "^5.1.2",
    "prompts": "^2.4.2",
    "serialport": "^10.4.0",
    "string-width": "^5.1.2",
    "telnet-client": "^2.0.6",
    "tftp": "^0.1.2",
    "ts-node": "^10.9.1",
    "win-select-folder": "^1.0.1"
  }
}
